{
  "name": "babel-preset-latest-node",
  "version": "3.0.1",
  "description": "babel preset latest for node 11 (Current), node 10 (Active LTS) node 8 (Maintenance LTS April 2019) or node 6 (EOL April 2019)",
  "keywords": ["babel-preset"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/babel-preset-latest-node.git",
  "homepage": "https://github.com/christophehurpeau/babel-preset-latest-node",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rm -Rf dist",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=lcov --coverageDirectory=docs/coverage/",
    "lint": "eslint lib/",
    "preversion": "yarn run lint && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "test": "jest",
    "version": "pob-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{package.json,.eslintrc.json}": [
      "prettier --parser json --write",
      "git add"
    ],
    "lib/**/*.json": ["prettier --parser json --write", "git add"],
    "lib/**/*.js": ["eslint --fix --quiet", "git add"]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/lib/**/__tests__/**/*.js",
      "<rootDir>/lib/**/*.test.js"
    ],
    "collectCoverageFrom": ["lib/**/*.js"],
    "moduleFileExtensions": ["js"]
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-async-generator-functions": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0",
    "@babel/plugin-proposal-unicode-property-regex": "^7.0.0",
    "@babel/plugin-syntax-async-generators": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-optional-catch-binding": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-exponentiation-operator": "^7.1.0",
    "@babel/plugin-transform-function-name": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@types/jest": "23.3.10",
    "eslint": "5.11.0",
    "eslint-config-pob": "25.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "husky": "1.2.1",
    "jest": "23.6.0",
    "jest-junit-reporter": "1.1.0",
    "lint-staged": "8.1.0",
    "pob-release": "4.6.3",
    "prettier": "1.15.3",
    "yarnhook": "0.3.0"
  }
}
